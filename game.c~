#include "my.h"

void	fill_up(int plateau[4][7])
{
  int	l;
  int	m;

  l = 0;
  while (m < 7)
    {
      if (m = 3)
	plateau[0][m]=1;
      else
	plateau[0][m]=0;
      m++;
    }
  l = 1;
  while (m < 7)
    {
      if (m = 2 || m = 3 || m = 4)
	plateau[0][m]=1;
      else
	plateau[0][m]=0;
      m++;
    }
  l = 2;
  while (m < 7)
    {
      if (m = 1 || m = 2 || m = 3 || m = 4 || m = 5)
	plateau[0][m]=1;
      else
	plateau[0][m]=0;
      m++;
    }
  l=3;
  while (m < 7)
    {
      if (m = 0 ||m = 1 || m = 2 || m = 3 || m = 4 || m = 5 || m = 6)
	plateau[0][m]=1;
      else
	plateau[0][m]=0;
      m++;
    }
  
